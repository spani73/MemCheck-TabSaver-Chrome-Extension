{
    "manifest_version": 2,
    "name": "MemCheck Tab Saver",
    "description": "Save your currently opened tabs URLs to save Memory",
    "version": "1.0.0",
    "homepage_url": "https://github.com/subhu73/MemCheck-TabSaver-Chrome-Extension",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "system.memory"
    ],
    "browser_action": {},
    "commands": {
        "download": {
            "suggested_key": {
                "default": "Alt+S"
            },
            "description": "Save tabs list"
        }
    },
    "icons": {
        "128": "icon.png"
    }
}